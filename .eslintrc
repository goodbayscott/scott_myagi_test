{
  "extends":                             "eslint-config-airbnb",
  "parser":                              "babel-eslint",
  "env":                                 {
    "browser":                           true,
    "node":                              true,
    "mocha":                             true
  },
  "globals"   :                          {
    "require"   :                        false,
    "module"    :                        false,
    "window"    :                        false,
    "document"  :                        false,
    "jest"      :                        false,
    "describe"  :                        false,
    "it"        :                        false,
    "expect"    :                        false,
    "beforeEach":                        false,
    "afterEach" :                        false,
    "before"    :                        false,
    "after"     :                        false,
    "Promise"   :                        false,
    "confirm"   :                        false,
    "context"   :                        false,
    "mixpanel"  :                        false
  },
  "rules":                               {
    "react/no-multi-comp":               0,
    "react/prop-types":                  0,
    "react/jsx-filename-extension":      0,
    "react/no-string-refs":              0,
    "react/prefer-stateless-function":   0,
    "import/default":                    0,
    "import/extensions":                 0,
    "import/no-duplicates":              0,
    "import/named":                      0,
    "import/namespace":                  0,
    "import/no-unresolved":              0,
    "import/no-named-as-default":        2,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export":      0,
    "import/first":                      0,
    "import-order-autofix/order":        0,
    "jsx-a11y/alt-text":                 0,
    "jsx-a11y/no-static-element-interactions": 1,
    "jsx-a11y/click-events-have-key-events": 1,
    "arrow-parens":                      0,
    "class-methods-use-this":            1,
    "comma-dangle":                      0,
    "global-require":                    0,
    "indent": [2, 2, {"SwitchCase":      1}],
    "no-return-assign":                  0,
    "no-console":                        0,
    "no-debugger":                       0,
    "no-alert":                          0,
    "no-unused-vars":                    1,
    "no-underscore-dangle":              0,
    "no-use-before-define":              1,
    "no-multiple-empty-lines":           0,
    "no-param-reassign":                 0,
    "id-length":                         0,
    "eol-last":                          0,
    "new-cap":                           0,
    "strict":                            0,
    "quotes":                           [1, "single", { "avoidEscape": true, "allowTemplateLiterals": true }],
    "react/jsx-no-literals":            [2, {"noStrings": true}]
  },
  "plugins":                             [
    "react", "import",
    "import"
  ],
  "settings":                            {
    "import/parser":                     "babel-eslint",
    "import/resolve":                    {
      "moduleDirectory":                 ["node_modules", "common-js", "."]
    }
  }
}
